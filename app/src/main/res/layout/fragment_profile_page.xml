<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Profile_page">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/profile_bg">

        <ImageButton
            android:id="@+id/back_btn_profile"
            android:layout_width="52dp"
            android:layout_height="49dp"
            android:background="@drawable/back_btn"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.072"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.034" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="My Profile"
            android:textColor="#000000"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/back_btn_profile"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.358"
            app:layout_constraintStart_toEndOf="@+id/back_btn_profile"
            app:layout_constraintTop_toTopOf="@+id/back_btn_profile"
            app:layout_constraintVertical_bias="0.4" />

        <TextView
            android:id="@+id/uname_profile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="64dp"
            android:text="Username"
            android:textColor="#292D32"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.461"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView3" />

        <TextView
            android:id="@+id/mail_profile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="email@email.com"
            android:textColor="#292D32"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/uname_profile"
            app:layout_constraintTop_toBottomOf="@+id/uname_profile"
            app:layout_constraintVertical_bias="0.0" />

        <ImageButton
            android:id="@+id/logout_btn_profile"
            android:layout_width="33dp"
            android:layout_height="37dp"
            android:layout_marginEnd="28dp"
            android:background="@drawable/baseline_logout_24"

            app:layout_constraintBottom_toBottomOf="@+id/mail_profile"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/uname_profile"
            app:layout_constraintTop_toTopOf="@+id/uname_profile"
            app:layout_constraintVertical_bias="1.0" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="136dp"
            android:text="Settings"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/back_btn_profile"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="@+id/back_btn_profile"
            app:layout_constraintTop_toBottomOf="@+id/back_btn_profile"
            app:layout_constraintVertical_bias="0.054" />

        <Button
            android:id="@+id/account_info_btn"
            android:layout_width="355dp"
            android:layout_height="60dp"
            android:layout_marginTop="116dp"
            android:backgroundTint="#E8E8E8"
            android:theme="@style/CustomButton"
            app:cornerRadius="10dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/logout_btn_profile"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/textView8"
            app:layout_constraintTop_toBottomOf="@+id/mail_profile"
            app:layout_constraintVertical_bias="0.0"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="21dp"
            android:layout_height="29dp"
            android:src="@drawable/user"
            app:layout_constraintBottom_toBottomOf="@+id/account_info_btn"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.06"
            app:layout_constraintStart_toStartOf="@+id/account_info_btn"
            app:layout_constraintTop_toTopOf="@+id/account_info_btn"
            app:layout_constraintVertical_bias="0.49" />

        <TextView
            android:id="@+id/account_info_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="14dp"
            android:text="Account Info"
            android:textColor="#06161C"
            android:textSize="14sp"
            android:typeface="sans"
            app:layout_constraintBottom_toBottomOf="@+id/account_info_btn"
            app:layout_constraintEnd_toEndOf="@+id/account_info_btn"
            app:layout_constraintHorizontal_bias="0.1"
            app:layout_constraintStart_toEndOf="@+id/imageView2"
            app:layout_constraintTop_toTopOf="@+id/account_info_btn"
            app:layout_constraintVertical_bias="0.22000003" />

        <ImageView
            android:id="@+id/arrow"
            android:layout_width="21dp"
            android:layout_height="29dp"
            android:src="@drawable/baseline_arrow_forward_ios_24"
            app:layout_constraintBottom_toBottomOf="@+id/account_info_btn"
            app:layout_constraintEnd_toEndOf="@+id/account_info_btn"
            app:layout_constraintHorizontal_bias="0.96"
            app:layout_constraintStart_toStartOf="@+id/account_info_btn"
            app:layout_constraintTop_toTopOf="@+id/account_info_btn"
            app:layout_constraintVertical_bias="0.516" />


        <Button
            android:id="@+id/password_profile"
            android:layout_width="355dp"
            android:layout_height="60dp"
            android:backgroundTint="#E8E8E8"
            android:theme="@style/CustomButton"
            app:cornerRadius="10dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/logout_btn_profile"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="@+id/textView8"
            app:layout_constraintTop_toBottomOf="@+id/account_info_btn"
            app:layout_constraintVertical_bias="0.014"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageView
            android:id="@+id/pwd_logo"
            android:layout_width="26dp"
            android:layout_height="28dp"
            android:src="@drawable/baseline_password_24"
            app:layout_constraintBottom_toBottomOf="@+id/password_profile"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.05"
            app:layout_constraintStart_toStartOf="@+id/password_profile"
            app:layout_constraintTop_toTopOf="@+id/password_profile"
            app:layout_constraintVertical_bias="0.49" />

        <TextView
            android:id="@+id/location"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="14dp"
            android:text="Password"
            android:textColor="#06161C"
            android:textSize="14sp"
            android:typeface="sans"
            app:layout_constraintBottom_toBottomOf="@+id/password_profile"
            app:layout_constraintEnd_toStartOf="@+id/arrow2"
            app:layout_constraintHorizontal_bias="0.1"
            app:layout_constraintStart_toEndOf="@+id/pwd_logo"
            app:layout_constraintTop_toTopOf="@+id/password_profile"
            app:layout_constraintVertical_bias="0.22000003" />

        <ImageView
            android:id="@+id/arrow2"
            android:layout_width="21dp"
            android:layout_height="29dp"
            android:src="@drawable/baseline_arrow_forward_ios_24"
            app:layout_constraintBottom_toBottomOf="@+id/password_profile"
            app:layout_constraintEnd_toEndOf="@+id/password_profile"
            app:layout_constraintHorizontal_bias="0.96"
            app:layout_constraintStart_toStartOf="@+id/password_profile"
            app:layout_constraintTop_toTopOf="@+id/password_profile"
            app:layout_constraintVertical_bias="0.516" />

        <Button
            android:id="@+id/privacy_policy"
            android:layout_width="355dp"
            android:layout_height="60dp"
            android:backgroundTint="#E8E8E8"
            android:theme="@style/CustomButton"
            app:cornerRadius="10dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/logout_btn_profile"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="@+id/textView8"
            app:layout_constraintTop_toBottomOf="@+id/password_profile"
            app:layout_constraintVertical_bias="0.014"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageView
            android:id="@+id/pp_logo"
            android:layout_width="26dp"
            android:layout_height="28dp"
            android:src="@drawable/baseline_policy_24"
            app:layout_constraintBottom_toBottomOf="@+id/privacy_policy"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.05"
            app:layout_constraintStart_toStartOf="@+id/privacy_policy"
            app:layout_constraintTop_toTopOf="@+id/privacy_policy"
            app:layout_constraintVertical_bias="0.49" />

        <TextView
            android:id="@+id/privacy_policy_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="14dp"
            android:text="Privacy Policy"
            android:textColor="#06161C"
            android:textSize="14sp"
            android:typeface="sans"
            app:layout_constraintBottom_toBottomOf="@+id/privacy_policy"
            app:layout_constraintEnd_toStartOf="@+id/arrow2"
            app:layout_constraintHorizontal_bias="0.1"
            app:layout_constraintStart_toEndOf="@+id/pp_logo"
            app:layout_constraintTop_toTopOf="@+id/privacy_policy"
            app:layout_constraintVertical_bias="0.22000003" />

        <ImageView
            android:id="@+id/arrow3"
            android:layout_width="21dp"
            android:layout_height="29dp"
            android:src="@drawable/baseline_arrow_forward_ios_24"
            app:layout_constraintBottom_toBottomOf="@+id/privacy_policy"
            app:layout_constraintEnd_toEndOf="@+id/privacy_policy"
            app:layout_constraintHorizontal_bias="0.96"
            app:layout_constraintStart_toStartOf="@+id/privacy_policy"
            app:layout_constraintTop_toTopOf="@+id/privacy_policy"
            app:layout_constraintVertical_bias="0.516" />

        <Button
            android:id="@+id/notif_btn"
            android:layout_width="355dp"
            android:layout_height="60dp"
            android:backgroundTint="#E8E8E8"
            android:theme="@style/CustomButton"
            app:cornerRadius="10dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/logout_btn_profile"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="@+id/textView8"
            app:layout_constraintTop_toBottomOf="@+id/privacy_policy"
            app:layout_constraintVertical_bias="0.014"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageView
            android:id="@+id/notif_logo"
            android:layout_width="26dp"
            android:layout_height="28dp"
            android:src="@drawable/baseline_notifications_24"
            app:layout_constraintBottom_toBottomOf="@+id/notif_btn"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.05"
            app:layout_constraintStart_toStartOf="@+id/notif_btn"
            app:layout_constraintTop_toTopOf="@+id/notif_btn"
            app:layout_constraintVertical_bias="0.49" />

        <TextView
            android:id="@+id/notif_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="14dp"
            android:text="Notifications"
            android:textColor="#06161C"
            android:textSize="14sp"
            android:typeface="sans"
            app:layout_constraintBottom_toBottomOf="@+id/notif_btn"
            app:layout_constraintEnd_toStartOf="@+id/arrow2"
            app:layout_constraintHorizontal_bias="0.1"
            app:layout_constraintStart_toEndOf="@+id/pp_logo"
            app:layout_constraintTop_toTopOf="@+id/notif_btn"
            app:layout_constraintVertical_bias="0.22000003" />

        <Switch
            android:id="@+id/switch1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:theme="@style/CustomSwitchStyle"
            android:thumbTint="#FFFFFF"
            android:trackTint="#47A920"
            app:layout_constraintBottom_toBottomOf="@+id/notif_btn"
            app:layout_constraintEnd_toEndOf="@+id/logout_btn_profile"
            app:layout_constraintHorizontal_bias="0.993"
            app:layout_constraintStart_toEndOf="@+id/notif_text"
            app:layout_constraintTop_toBottomOf="@+id/arrow3"
            app:layout_constraintVertical_bias="0.679" />

        <Button
            android:id="@+id/fback"
            android:layout_width="362dp"
            android:layout_height="137dp"
            android:layout_marginTop="48dp"
            android:backgroundTint="#E8E8E8"
            android:fontFamily="@font/dm_sans"
            android:text="How can we help you?"
            android:textColor="#06161C"
            android:textSize="20sp"
            android:textStyle="bold"
            android:theme="@style/CustomButton"
            android:typeface="normal"
            app:cornerRadius="10dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/notif_btn"
            app:layout_constraintHorizontal_bias="0.571"
            app:layout_constraintStart_toStartOf="@+id/notif_btn"
            app:layout_constraintTop_toBottomOf="@+id/notif_btn"
            app:layout_constraintVertical_bias="0.074" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="105dp"
            android:layout_height="104dp"
            android:src="@drawable/sample_profile"
            app:layout_constraintBottom_toTopOf="@+id/textView8"
            app:layout_constraintEnd_toStartOf="@+id/uname_profile"
            app:layout_constraintHorizontal_bias="0.086"
            app:layout_constraintStart_toStartOf="@+id/back_btn_profile"
            app:layout_constraintTop_toBottomOf="@+id/back_btn_profile"
            app:layout_constraintVertical_bias="0.212" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>